<h1>Table User</h1>

<button>
    <a href="/add-user">
        Tambah User
    </a>
</button>

<table border="1">
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Gender</th>
      <th>Action</th>

    </tr>
    
    <% dataUsers.forEach(el => { %>
        <tr>
            <td><%= el.first_name %></td>
            <td><%= el.last_name %></td>
            <td><%= el.email %></td>
            <td><%= el.gender %></td>
            <td>
                <a href="/form-edit/<%=el.id%>" >
                    Edit
                </a>
                ||
                <a href="/delete-user/<%=el.id%>">
                    Delete
                </a>
            </td>
        </tr>
    <% }) %>
  </table>